{
  "rules": {
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": {
          "multiline": true,
          "minProperties": 4,
          "consistent": true
        },
        "ObjectPattern": {
          "multiline": true,
          "minProperties": 4,
          "consistent": true
        }
      }
    ],
    "object-property-newline": [
      "error",
      {
        "allowAllPropertiesOnSameLine": true
      }
    ]
  }
}
